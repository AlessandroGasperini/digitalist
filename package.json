    "start": "npm-run-all -p watch:css start:js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.2",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5"
  }
}